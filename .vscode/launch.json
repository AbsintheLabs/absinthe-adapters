{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug",
      "type": "node",
      "request": "launch",
      "program": "pnpm start ${workspaceFolder}/adapters/uniswap-v2/tests/config/uniswap-v2.absinthe.json --reset-state",
      "cwd": "${workspaceFolder}",
      "runtimeArgs": ["--no-deprecation", "--no-warnings", "--enable-source-maps"],
      "skipFiles": ["<node_internals>/**", "${workspaceFolder}/**/node_modules/**/*.js"],
      "sourceMaps": true,
      "outFiles": ["${workspaceFolder}/dist/src/**/*.js"]
    }
  ]
}

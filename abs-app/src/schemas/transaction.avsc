{
    "type": "record",
    "namespace": "network.absinthe.adapters",
    "name": "TransactionEvent",
    "doc": "On-chain transaction snapshot (swap, transfer, trade, etc.).",
    "fields": [
        {
            "name": "base",
            "type": "network.absinthe.adapters.Base"
        },
        {
            "name": "eventType",
            "type": {
                "type": "enum",
                "name": "EventType",
                "symbols": [
                    "transaction"
                ]
            }
        },
        {
            "name": "tokens",
            "type": {
                "type": "array",
                "items": {
                    "type": "record",
                    "name": "TokenAmount",
                    "fields": [
                        {
                            "name": "token",
                            "type": {
                                "type": "record",
                                "name": "Token",
                                "fields": [
                                    {"name": "address", "type": "string"},
                                    {"name": "symbol", "type": "string"},
                                    {"name": "decimals", "type": "int"}
                                ]
                            }
                        },
                        {"name": "amount", "type": "string"},
                        {"name": "amountIn", "type": "string"},
                        {"name": "amountOut", "type": "string"}
                    ]
                }
            },
            "default": []
        },
        {
            "name": "rawAmount",
            "type": "string"
        },
        {
            "name": "displayAmount",
            "type": "double"
        },
        {
            "name": "unixTimestampMs",
            "type": "long"
        },
        {
            "name": "txHash",
            "type": "string"
        },
        {
            "name": "logIndex",
            "type": "int"
        },
        {
            "name": "blockNumber",
            "type": "int"
        },
        {
            "name": "blockHash",
            "type": "string"
        }
    ]
}
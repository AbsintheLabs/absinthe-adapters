{
  "name": "absinthe-adapters",
  "description": "Monorepo for Absinthe protocol adapters",
  "private": true,
  "version": "0.0.1",
  "workspaces": [
    "projects/*",
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r build",
